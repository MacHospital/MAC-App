{
  "expo": {
    "name": "mac-hospitals",
    "slug": "mac-hospitals",
    "version": "1.0.0",
    "scheme": "mac-hospitals",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apostleapp.machospitals",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "expo": {
      "splash": {
        "image": "./assets/splashscreen_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "package": "com.apostleapp.machospitals",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FE6249"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "061848b8-e3b0-4678-b85a-a1e635520796"
      }
    },
    "owner": "steph4n"
  }
}
